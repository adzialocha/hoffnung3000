{
  "name": "hoffnung3000",
  "version": "2.0.0",
  "description": "Platform for anonymous, decentralized and self-curated festivals",
  "main": "server/index.js",
  "private": true,
  "contributors": [
    "adzialocha"
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": "~10.12.0",
    "npm": "~6.6.0"
  },
  "scripts": {
    "assets:build": "npm run assets:clear && webpack --mode production --progress",
    "assets:clear": "rimraf ./static",
    "assets:watch": "npm run assets:clear && webpack --mode development --watch --progress",
    "build": "npm run assets:build && npm run server:build",
    "clear": "npm run assets:clear && npm run server:clear",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:create": "sequelize migration:generate --name untitled",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "db:seed:create": "sequelize seed:generate --name untitled",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ignore-path .gitignore ./",
    "lint:scss": "stylelint ./app/styles",
    "postinstall": "npm run build",
    "serve": "npm run server:watch",
    "server:build": "babel ./server --out-dir ./dist && cp ./server/index.pug ./dist",
    "server:clear": "rimraf ./dist",
    "server:start": "cross-env NODE_ENV=production node ./dist/index.js",
    "server:watch": "nodemon --watch ./server --watch ./common --exec cross-env NODE_ENV=development babel-node server/index.js --source-maps inline",
    "start": "npm run server:start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/adzialocha/hoffnung3000.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
